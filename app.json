{
  "expo": {
    "name": "Los Pollos Hermanos",
    "slug": "los-pollos-hermanos",
    "version": "1.0.0",
    "router": {
      "root": "src/app"
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "favicon": "./assets/images/favicon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.emiliohb.mobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1c0f0a",
          "dark": {
            "backgroundColor": "#1c0f0a"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bc5e7ff1-8b04-4d4d-bc45-ba7d6085e72f"
      }
    },
    "owner": "emiliohb"
  }
}
